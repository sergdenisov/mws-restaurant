{
  "extends": [
    "eslint:recommended",
    "google",
    "plugin:prettier/recommended"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "commonjs": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": "error",
    "no-console": ["error", {"allow": ["error"]}]
  },
  "plugins": ["prettier"]
}
